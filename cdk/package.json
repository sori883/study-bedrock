{
  "name": "aws-cdk-template",
  "version": "0.1.0",
  "private": "true",
  "bin": {
    "aws-cdk-template": "bin/entrypoint.ts"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:update": "jest -u",
    "cdk": "cdk",
    "synth": "cdk synth",
    "deploy": "cdk deploy *",
    "deploy:never": "cdk deploy '*' --require-approval never",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "aws-cdk-lib": "2.210.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/aws-lambda": "^8.10.155",
    "@types/jest": "^30.0.0",
    "@types/node": "22.17.1",
    "aws-cdk": "2.1024.0",
    "dotenv": "^17.2.3",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.2"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22.18.0"
  }
}
